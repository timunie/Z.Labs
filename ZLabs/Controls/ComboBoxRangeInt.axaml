<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             Name="Control"
             x:Class="ZLabs.Controls.ComboBoxRangeInt">

    <ComboBox SelectedIndex="{Binding ElementName=Control, Path=SelectedIndex}" FontSize="18"
              FontFamily="{StaticResource MontserratRegular}" Width="250"
              Items="{Binding ElementName=Control, Path=Items}">
    </ComboBox>
</UserControl>
